
@{
    ViewBag.Title = "Index";
}
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <title>Login </title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!---CSS-->
    <link rel="icon" type="image/png" href="~/Content/favicon.ico" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/font_/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/icon-font.min.css" rel="stylesheet" />
    <link href="~/Content/Login/animate.css" rel="stylesheet" />
    <link href="~/Content/Login/hamburgers.min.css" rel="stylesheet" />
    <link href="~/Content/Login/animsition.min.css" rel="stylesheet" />
    <link href="~/Content/Login/select2.min.css" rel="stylesheet" />
    <link href="~/Content/Login/daterangepicker.css" rel="stylesheet" />
    <link href="~/Content/main.css" rel="stylesheet" />
    <link href="~/Content/util.css" rel="stylesheet" />

</head>
<body>
    <div class="limiter">
        <div class="container-login100" style="background-image: url(../../Content/bg-01.jpg);">
            <div class="wrap-login100 p-t-30 p-b-50">
                <span class="login100-form-title p-b-41">
                    Bienvenido a Promotec
                </span>
                <form class="login100-form validate-form p-b-33 p-t-5">

                    <div class="wrap-input100 validate-input" data-validate="Enter username">
                        <input class="input100" id="name" type="text" name="username" placeholder="Ingrese Dni">
                        <span class="focus-input100" data-placeholder="&#xe82a;"></span>
                    </div>

                    <div class="wrap-input100 validate-input" data-validate="Enter password">
                        <input class="input100" id="pass" type="password" name="pass" placeholder="Ingrese contraseña">
                        <span class="focus-input100" data-placeholder="&#xe828;"></span>
                    </div>

                    <div class="container-login100-form-btn m-t-32">
                        <input type="button" class="btn btn-primary" value="Login" onclick="a()">
                            <a style="margin-left:20px" href="/Registro" class="btn btn-warning">Registrarse</a>
                    </div>

                </form>
            </div>
        </div>
    </div>


    <div id="dropDownSelect1"></div>
    @Html.Hidden("RedirectTo", Url.Action("Index", "Home"));
    <!---SCRIPTS-->
    <script src="~/Scripts/Login/jquery-3.2.1.min.js"></script>
    <script src="~/Scripts/Login/animsition.min.js"></script>
    <script src="~/Scripts/Login/popper.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/Login/select2.min.js"></script>
    <script src="~/Scripts/Login/moment.min.js"></script>
    <script src="~/Scripts/Login/daterangepicker.js"></script>
    <script src="~/Scripts/Login/countdowntime.js"></script>
    <script src="~/Scripts/main.js"></script>
    <script src="~/Scripts/redirigir.js"></script>

</body>
</html>